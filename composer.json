{
    "name": "b7s/laravel-queue-flow",
    "description": "A fluent Laravel Queue wrapper, simplifying queue management without creating separate Job classes",
    "type": "library",
    "keywords": ["laravel", "queue", "parallite", "jobs", "async", "fluent", "wrapper", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Tenorio",
            "email": "b7s@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "laravel/serializable-closure": "^1.3|^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "B7s\\QueueFlow\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "B7s\\QueueFlow\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "B7s\\QueueFlow\\QueueFlowServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
